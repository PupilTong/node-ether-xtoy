{
  "name": "ether-xtoy",
  "version": "0.0.6",
  "description": "a ether_aton, ether_ntoa bindings for Node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "install": "prebuild-install --runtime napi --tag-prefix '' || node-gyp rebuild",
    "build": "node-gyp rebuild",
    "build:dev": "node-gyp rebuild --debug",
    "test": "mocha src/tests"
  },
  "keywords": [
    "ethernet",
    "ether_ntoa",
    "ether_aton"
  ],
  "author": "pupilTong",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PupilTong/node-ether-xtoy.git"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "src/ether_xtoy.cpp",
    "binding.gyp"
  ],
  "gypfile":true,
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^4.0.0",
    "prebuild-install": "^7.0.0"
  },
  "devDependencies": {
    "mocha": "^9.0.0",
    "node-addon-api": "^4.0.0",
    "node-gyp": "^8.0.0",
    "should": "^13.2.3"
  },
  "bugs":{
    "url": "https://github.com/PupilTong/node-ether-xtoy/issues"
  },
  "homepage": "https://github.com/PupilTong/node-ether-xtoy#readme",
  "binary":{
    "napi_versions":[
      3
    ]
  }
}
